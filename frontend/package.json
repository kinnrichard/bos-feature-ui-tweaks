{
	"name": "bos-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "DEBUG=bos:* vite dev",
		"dev:quiet": "vite dev",
		"dev:zero": "node ../scripts/zero-dev-server.js",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:mocked": "TEST_STRATEGY=mocked playwright test",
		"test:real-db": "TEST_STRATEGY=real_db USE_REAL_DB=true playwright test",
		"test:unit": "playwright test --project=unit",
		"test:integration": "playwright test --project=integration",
		"test:e2e": "playwright test tests/e2e --project=e2e-chromium",
		"test:vitest": "vitest",
		"test:vitest:run": "vitest run",
		"test:vitest:ui": "vitest --ui",
		"test:vitest:coverage": "vitest --coverage",
		"test:generator": "vitest run tests/generator/generator-integration.spec.ts",
		"test:debug": "DEBUG=true playwright test --debug",
		"test:headed": "playwright test --headed",
		"test:report": "playwright show-report",
		"build:test": "vite build --mode test --config vite.config.test.ts",
		"preview:test": "vite preview --mode test --config vite.config.test.ts",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier --check .",
		"test:epic-015": "vitest run --config vitest.epic-015.config.ts",
		"test:epic-015:watch": "vitest watch --config vitest.epic-015.config.ts",
		"test:epic-015:coverage": "vitest run --config vitest.epic-015.config.ts --coverage",
		"test:epic-015:security": "vitest run --config vitest.epic-015.config.ts src/lib/utils/debug/__tests__/epic-015-security.test.ts",
		"test:epic-015:performance": "vitest run --config vitest.epic-015.config.ts src/lib/utils/debug/__tests__/epic-015-performance.test.ts",
		"test:epic-015:integration": "vitest run --config vitest.epic-015.config.ts src/lib/utils/debug/__tests__/epic-015-integration.test.ts",
		"test:epic-015:migration": "vitest run --config vitest.epic-015.config.ts src/lib/utils/debug/__tests__/epic-015-migration.test.ts",
		"test:epic-015:report": "vitest run --config vitest.epic-015.config.ts --reporter=html --reporter=json --reporter=verbose"
	},
	"devDependencies": {
		"@eslint/js": "^9.30.0",
		"@lucide/svelte": "^0.515.0",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/debug": "^4.1.12",
		"@types/eslint": "^9.0.0",
		"@types/fast-redact": "^3.0.4",
		"@types/node": "^24.0.14",
		"@types/qs": "^6.14.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^5.35.4",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^1.3.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.8.2",
		"@melt-ui/svelte": "^0.86.6",
		"@rocicorp/zero": "^0.21.2025070200",
		"axios": "^1.10.0",
		"bits-ui": "^2.9.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"debug": "^4.4.1",
		"fast-redact": "^3.5.0",
		"libphonenumber-js": "^1.11.18",
		"lucide-svelte": "^0.536.0",
		"qs": "^6.14.0",
		"shadcn-svelte": "^1.0.6",
		"tailwind-merge": "^3.3.1"
	}
}
